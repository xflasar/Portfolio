{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "plugin:jest/recommended",
      "standard"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "jest"
  ],
  "rules": {
      "no-console": 0,
      "no-empty": 0,
      "no-irregular-whitespace": 0
  },
  "settings": {
      "react": {
        "version": "detect"
      },
      "jest": {
        "version": "detect"
      }
  }
}